    "Filter": {
        "id_name": "Filter",
        "icon_type": "Dataset Operations",
        "name": "",
        "grid_config": " \"attributes attributes\" \" values values \" \" condition contains_logic \" \" attribute_logic attribute_logic \" \" name id_name \" ",
        "attributes": {
            "textlist": "",
            "show_attributes": true,
            "required": true,
            "help": "Attribute to apply the filter."
        },
        "values": {
            "textlist": "",
            "required": true,
            "help": "List of values ​​to filter, separated by comma. Special filters: _null, _today, _yesterday, _this_week, _last_week, _this_month, _last_month, _this_year, _last_year, _sunday, _monday, _tuesday, _wednesday, _thursday, _friday and _saturday"
        },
        "condition": {
            "select": [
                "contains",
                "not_contains",
                "greater_than",
                "greater_equal",
                "less_than",
                "less_equal",
                "not_equal_to",
                "equal_to"
            ],
            "help": ""
        },
        "contains_logic": {
            "select": ["or", "and"],
            "help": "If the filter is of type contains or not_contains, the input values ​​can be tied to an OR / AND logic."
        },
        "attribute_logic": {
            "select": ["or", "and"],
            "help": "Logic to tie the filter with multiple attributes."
        },
        "component_type": "process",
        "input_dim": 1,
        "help": "Performs filters on tabular data and text.",
        "help_link": "https://axondata.atlassian.net/wiki/spaces/wiki/pages/191201404/Filter"
    },

